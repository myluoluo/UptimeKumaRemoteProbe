{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "dd/MM/yyyy HH:mm:ss",
        "UseUtcTimestamp": false,
        "JsonWriterOptions": {
          "Indented": false
        }
      }
    }
  },
  "Configurations": {
    "MonitorsApi": "http://localhost:5050/monitors",
    "ProbeName": "Home",
    "BasePushUri": "http://localhost:3001/api/push/",
    "UpDependency": "192.168.1.1",
    "Timeout": 1000,
    "Delay": 5000
  }
}
